    "import glob\n",
    "\n",
    "files = glob.glob(\"./hello/*_*.txt\")\n",
    "\n",
    "n = 0\n",
    "for file in files:\n",
    "    sid = file.split(\"/\")[2].split(\"_\")[0]\n",
    "    n += 1\n",
    "    line = open(file, 'r').read().strip()\n",
    "    print(str(n).zfill(2), sid, line, sep=' | ')\n",
    "    print(\"-\" * 80)"
   ]
